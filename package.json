{
  "name": "huge-table",
  "version": "6.5.2",
  "description": "Table component to handle huge sets of data, based on Facebook's FixedDataTable",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha",
    "cleanup": "rm -rf dist",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --port 8084 --inline --content-base example/public/ --history-api-fallback",
    "babel": "mkdir dist/ && babel src --out-dir dist",
    "less": "lessc src/HugeTable.less dist/HugeTable.css",
    "release": "npm run cleanup && npm run babel && webpack --config ./webpack.release.config.js",
    "test-circle": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/file.xml mocha test --reporter mocha-junit-reporter",
    "npmrc": "printf \"//registry.npmjs.org/:_authToken=$NPM_AUTH_TOKEN\" > .npmrc"
  },
  "author": "Yacine Rezgui (@yrezgui)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/import-io/huge-table"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fixed-data-table": "^0.6.0",
    "fixed-data-table-2": "^0.7.10",
    "font-awesome": "^4.7.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-portal": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap": "^3.3.5",
    "chai": "^3.0.0",
    "clean-css": "^3.4.4",
    "css-loader": "^0.14.4",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.1",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.10.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "jsdom": "^5.4.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.6.1",
    "node-libs-browser": "^0.5.2",
    "serve": "^1.4.0",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0",
    "skin-deep": "^0.14.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
